# Ethernet configuration for ESP32-P4-NANO
# IP101GRI PHY chip with RMII interface
CONFIG_ETHERNET_MDC_GPIO=31
CONFIG_ETHERNET_MDIO_GPIO=52
CONFIG_ETHERNET_PHY_RST_GPIO=51
CONFIG_ETHERNET_PHY_ADDR=1
CONFIG_ETHERNET_PHY_IP101=y

# Wi-Fi AP configuration
CONFIG_EXAMPLE_WIFI_SSID="ESP32-P4-NANO-WiFi"
CONFIG_EXAMPLE_WIFI_PASSWORD="12345678"